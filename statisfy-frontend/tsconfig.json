{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "jsx": "react-jsx", 
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true
    },
    "include": [
      "src"
    ]
  }
  